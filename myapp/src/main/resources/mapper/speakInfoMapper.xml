<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.example.myapp.DAO.speakInfoMapper">
    <select id="getContent" resultType="String">
        select content from speakInfo where id=1;
    </select>
    <insert id="setContent" parameterType="String">
        update speakinfo set content=#{param1} where id=1;
    </insert>
</mapper>